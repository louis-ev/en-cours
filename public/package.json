{
  "name": "encours",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "dev": "npm-run-all --parallel watchify",
    "lint": "eslint --ext .js,.vue src",
    "build": "cross-env NODE_ENV=production browserify -g envify -e src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      "node-lessify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^1.6.0",
    "@xkeshi/vue-qrcode": "^1.0.0",
    "alertify.js": "^1.0.12",
    "axios": "^0.18.0",
    "express-ws": "^4.0.0",
    "i18n": "^0.8.3",
    "imagetracerjs": "^1.2.5",
    "instascan": "^1.0.0",
    "jquery": "^3.3.1",
    "moment": "^2.29.4",
    "node-lessify": "^0.1.5",
    "node-sass": "^4.11.0",
    "portal-vue": "^1.5.1",
    "quill": "^1.3.6",
    "quill-render": "^1.0.5",
    "reconnectingwebsocket": "^1.0.0",
    "recordrtc": "^5.5.4",
    "rich-text": "^3.1.0",
    "rtcmulticonnection": "^3.6.8",
    "sharedb": "^1.0.0-beta.18",
    "slugg": "^1.2.1",
    "socket.io": "^2.2.0",
    "spark-md5": "^3.0.0",
    "store": "^2.0.12",
    "underscore": "^1.9.1",
    "vue": "^2.5.22",
    "vue-i18n": "^8.8.1",
    "vue-template-compiler": "^2.6.4",
    "vue2-animate": "^2.1.0",
    "vue2-editor": "^2.6.6",
    "websocket-json-stream": "0.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.3.0",
    "browserify": "^16.2.3",
    "browserify-css": "^0.14.0",
    "browserify-hmr": "^0.3.7",
    "cross-env": "^5.2.0",
    "envify": "^4.1.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "proxyquireify": "^3.0.1",
    "uglify-es": "^3.3.10",
    "vueify": "^9.4.1",
    "watchify": "^3.4.0"
  }
}
